<% include('../layouts/header.ejs')%>

<h1>DashBoard</h1>
<!-- <p><%- users     %></p> -->
<table width="50%">

    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Image</th>
        <th>Verified</th>
    </tr>
    <%
     if(users.length>0){
         for(let i = 0; i<users.length; i++){
         %>
            <tr>

                <td><%= users[i].name %></td>
                <td><%= users[i].email %></td>
                <td><%= users[i].mobile %></td>
                <td><img src="/userImages/<%= users[i].image %>" height="200px" width="200px" alt="<%= users[i].image %>"></td>
                <td><%= users[i].is_verified %></td>

            </tr>


        <%

         }


     }else{
         %>
         <tr>
             <td colspan="5"> Users Not found </td>
         </tr>
         <%
     }



    %>

</table>

<a href = "/admin/home">Go To Home </a>

<% include('../layouts/footer.ejs')%>